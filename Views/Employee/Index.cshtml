@model System.Collections.Generic.IList<shop.Models.Employee>
@{
    ViewData["Title"] = "Index";
}

<h1>Employee List</h1>
<table class="table">
    <th>Employee No</th>
    <th>Employee Name</th>
    @foreach(var item in Model) {
        <tr>
            <td>@item.EmployeeNo</td>
            <td>@item.EmployeeName</td>
        </tr>
    }
</table>